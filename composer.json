{
    "name": "carpediem/mattermost-monolog",
    "description": "Mattermost monolog integration",
    "homepage": "https://github.com/carpediem/mattermost-monolog",
    "version": "1.0.0",
    "keywords": [
        "mattermost",
        "php",
        "monolog",
        "monolog-handler",
        "monolog-formatter",
        "php7"
    ],
    "type": "library",
    "require": {
    	"php": ">=5.6.0",
        "monolog/monolog": "^1.23",
        "carpediem/mattermost-webhook": "^1.0.0 || ^2.0.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.0",
        "phpunit/phpunit": "^6.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Carpediem Developper",
            "email": "dev@carpediem.fr",
            "homepage": "https://carpediem.github.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Carpediem\\Mattermost\\Monolog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Carpediem\\Mattermost\\Monolog\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit --coverage-text; php-cs-fixer fix -v --diff --dry-run --allow-risky=yes;",
        "phpunit": "phpunit --coverage-text",
        "phpcs": "php-cs-fixer fix -v --diff --dry-run --allow-risky=yes;"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
